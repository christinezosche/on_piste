
<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
    Email: <%= text_field_tag "email" %><br>
    Password: <%= text_field_tag "password" %>
    <%= submit_tag "Sign In" %>
<% end %>

<%= button_to "Log In with Google", '/auth/google_oauth2' %>

<%= form_for :user do |f|  %>
    <%= f.text_field :email %>
    <%= f.password_field :password %>
    <%= f.submit "Log In" %>
<% end %>